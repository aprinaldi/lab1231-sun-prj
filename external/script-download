wget http://www.vlfeat.org/download/vlfeat-0.9.19.tar.gz
wget http://cs.brown.edu/~pff/segment/segment.tgz
wget http://www.inf.ethz.ch/personal/ladickyl/robustpn.zip
wget http://graphics.stanford.edu/projects/densecrf/unary/probimage.zip
wget http://graphics.stanford.edu/projects/densecrf/textonboost/classification.zip

tar -zxvf vlfeat-0.9.19.tar.gz
tar -zxvf segment.tgz
mv segment pff-segment
unzip robustpn.zip -d robustpn
unzip probimage.zip -d prob-image
unzip classification.zip -d textonboost

rm *.tar.gz
rm *.tgz
rm *.zip